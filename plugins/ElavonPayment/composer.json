{
    "name": "Zuluru/ElavonPayment",
    "description": "Elavon payment processing plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2.0",
        "cakephp/cakephp": ">=3.8.4",
		"ext-curl": "*",
		"ext-simplexml": "*"
	},
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "ElavonPayment\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElavonPayment\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
